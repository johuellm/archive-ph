{
  "manifest_version": 3,
  "name": "Archive.ph oneclick",
  "description": "Open up the oldest archive.ph version from a link",
  "version": "1.0",
  "developer": {
    "name": "Joschka HÃ¼llmann",
    "url": "https://github.com/johuellm/archive-ph"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "archive-ph@joschka-huellmann.net",
      "strict_min_version": "109.0"
    }
  },
  "permissions": ["contextMenus"],
  "background": {
    "scripts": ["background.js"]
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png"
  }
}
